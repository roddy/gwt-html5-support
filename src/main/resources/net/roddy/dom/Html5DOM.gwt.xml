<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Html5DOM'>
  <inherits name='com.google.gwt.core.Core' />
  <inherits name='com.google.gwt.canvas.dom.DOM' />
  <inherits name='com.google.gwt.media.dom.DOM' />
  <inherits name='com.google.gwt.safehtml.SafeHtml' />
  <inherits name='com.google.gwt.useragent.UserAgent' />
  <inherits name='com.google.gwt.dom.DOM' />

  <source path='client' />
  <source path='shared' />

  <replace-with class="com.google.gwt.dom.client.Html5DOMImplIE8">
      <when-type-is class="com.google.gwt.dom.client.DOMImplIE8" />
  </replace-with>

  <replace-with class="com.google.gwt.dom.client.Html5Document">
      <when-type-is class="com.google.gwt.dom.client.Document" />
  </replace-with>
</module>
